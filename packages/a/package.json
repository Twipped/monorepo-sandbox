{
  "name": "@example/monorepo-test-a",
  "version": "3.0.6",
  "description": "First workspace test package",
  "main": "dist/index.cjs",
  "author": "Jocelyn Badgley <jocelyn.badgley@twipped.com>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist/* && npm run build:mjs && npm run build:cjs && npm run build:types",
    "build:cjs": "babel src --out-dir dist --out-file-extension .cjs --ignore '**/*.test.js' --ignore '**/__test__/*'",
    "build:mjs": "ESM=1 babel src --out-dir dist --out-file-extension .mjs --ignore '**/*.test.js' --ignore '**/__test__/*'",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "NODE_ENV=test jest",
    "test-watch": "NODE_ENV=test jest --watch",
    "test-types": "tsc --noEmit --checkJs"
  },
  "type": "module",
  "engines": {
    "node": "^18.12.0 || ^20.9.0"
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "webpack": "./src/index.js"
    },
    "./MockProvider": {
      "import": "./dist/MockProvider.mjs",
      "require": "./dist/MockProvider.cjs",
      "webpack": "./src/MockProvider.js"
    }
  },
  "typesVersions": {
    "*": {
      "": [
        "./dist/index.d.ts"
      ],
      "MockProvider": [
        "./dist/MockProvider.d.ts"
      ],
      "dist/*": [
        "./dist/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twipped/monorepo-sandbox.git",
    "directory": "packages/a"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/?(*.)+(spec|test).js"
    ],
    "transform": {
      "\\.js?$": "babel-jest"
    },
    "automock": false,
    "clearMocks": true
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@types/react": "^18.2.46",
    "babel-plugin-add-import-extension": "^1.6.0",
    "eslint": "^8.12.0",
    "prop-types": "latest",
    "react": "latest",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
